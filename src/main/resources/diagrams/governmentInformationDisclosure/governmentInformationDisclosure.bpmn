  <process id="governmentInformationDisclosure" name="政府信息公开new" isExecutable="true">
    <startEvent id="startevent1" name="Start" activiti:initiator="startUserId" activiti:formKey="governmentInformationDisclosure/applicationForm">
      <documentation>启动流程，显示申请人表单</documentation>
    </startEvent>
    <userTask id="usertask2" name="【单位回复意见】${applicationName}" activiti:assignee="${assignee}" activiti:formKey="governmentInformationDisclosure/replyForm">
      <documentation>单位回复意见</documentation>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${accountService.resolveUsername(assigneeList,&quot;,&quot;)}" activiti:elementVariable="assignee">
        <completionCondition>${nrOfActiveInstances==0}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <userTask id="usertask3" name="【审核办结】${applicationName}" activiti:assignee="${startUserId}" activiti:formKey="governmentInformationDisclosure/reviewForm"></userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <serviceTask id="servicetask1" name="创建申请单" activiti:expression="${governmentInformationDisclosureService.save(applicationName,citizenName, citizenWorkunit,citizenCertificate, citizenCertificateID,citizenTelphone, citizenZipcode,citizenAddress, citizenFax, citizenEmail,groupName, groupID, groupDelegate,groupDelegateName, groupDelegateTelphone,groupDelegateFax, groupDelegateAddress,groupDelegateEmail, applicationTime,submitDepartment, description, purpose,mode, obtainMode)}" activiti:resultVariableName="result"></serviceTask>
    <serviceTask id="servicetask3" name="保存审核意见" activiti:expression="${1==1}" activiti:resultVariableName="result"></serviceTask>
    <sequenceFlow id="flow21" sourceRef="servicetask3" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow22" sourceRef="usertask3" targetRef="servicetask3"></sequenceFlow>
    <sequenceFlow id="flow23" sourceRef="servicetask1" targetRef="usertask2"></sequenceFlow>
    <serviceTask id="servicetask4" name="保存单位意见" activiti:expression="${governmentInformationDisclosureService.addReply(result,reply)}" activiti:resultVariableName="result"></serviceTask>
    <sequenceFlow id="flow24" sourceRef="usertask2" targetRef="servicetask4"></sequenceFlow>
    <sequenceFlow id="flow25" sourceRef="servicetask4" targetRef="usertask3"></sequenceFlow>
    <scriptTask id="scripttask1" name="设置选择项默认值" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>if(execution.getVariable("mode")==null){
execution.setVariable("mode", "")
}
if(execution.getVariable("obtainMode")==null){
execution.setVariable("obtainMode", "")
}</script>
    </scriptTask>
    <sequenceFlow id="flow26" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <sequenceFlow id="flow27" sourceRef="scripttask1" targetRef="servicetask1"></sequenceFlow>
    <textAnnotation id="textannotation1">
      <text>单位回复意见</text>
    </textAnnotation>
    <association id="association1" sourceRef="textannotation1" targetRef="usertask2"></association>
    <textAnnotation id="textannotation2">
      <text>审核办结</text>
    </textAnnotation>
    <association id="association2" sourceRef="textannotation2" targetRef="usertask3"></association>
    <textAnnotation id="textannotation3">
      <text>填写申请表单</text>
    </textAnnotation>
    <association id="association3" sourceRef="textannotation3" targetRef="startevent1"></association>
    <textAnnotation id="textannotation4">
      <text>开始表单中的mode与obtainMode复选框如果未选择，则会缺少流程变量，用脚本来设置该变量，防止流程出错。</text>
    </textAnnotation>
    <association id="association4" sourceRef="textannotation4" targetRef="scripttask1"></association>
  </process>
